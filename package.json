{
  "name": "rj-portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rehooks/window-scroll-position": "github:rehooks/window-scroll-position",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dotenv": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.16.4",
    "fuse.js": "^3.3.0",
    "google-map-react": "^1.1.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-pure-render": "^1.0.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
