    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "GENERATE_SOURCEMAP=false npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.1",
    "gh-pages": "^2.0.1",
    "postcss-cli": "^6.0.1",
    "stylelint-config-recommended": "^2.1.0",
    "tailwindcss": "^0.7.2"
  }
